{
  "name": "GHRVis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "3.1.1",
    "bootstrap": "4.6.0",
    "bootstrap-vue": "2.21.2",
    "core-js": "3.6.5",
    "csv-parser": "3.0.0",
    "d3": "6.7.0",
    "path-data-polyfill": "1.0.3",
    "path-intersection": "2.2.1",
    "point-in-svg-polygon": "1.0.25",
    "topojson-client": "3.1.0",
    "vue": "2.6.11",
    "vue-router": "3.2.0",
    "vuex": "3.4.0",
    "webcola": "3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "6.2.2",
    "lint-staged": "9.5.0",
    "prettier": "2.2.1",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
